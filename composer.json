{
    "name": "kami/booking-bundle",
    "type": "symfony-bundle",
    "keywords": ["booking", "availability", "calendar"],
    "homepage": "https://github.com/kamilabs/BookingBundle",
    "description": "Adds some booking functionality to your Symfony project",
    "license": "MIT",
    "authors": [
        {
            "name": "Iakov Mishchenko",
            "email": "iakov@kamilabs.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php":">=5.3.2",
        "symfony/symfony" : ">2.8",
        "symfony/framework-bundle": ">2.8",
        "twig/twig": "~1.3",
        "twig/extensions": "~1.4",
        "doctrine/orm": "~2.5",
        "doctrine/doctrine-bundle": "~1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5",
        "doctrine/doctrine-fixtures-bundle": "dev-master"
    },
    "suggest": {
        "doctrine/mongodb-odm": "1.0.*@dev"
    },
    "autoload": {
        "psr-0": { "Kami\\BookingBundle": "" }
    },
    "target-dir": "Kami/BookingBundle"
}
